<?xml version="1.0" encoding="ISO-8859-1"?>
<system xmlns="http://www.eiffel.com/developers/xml/configuration-1-0-0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.eiffel.com/developers/xml/configuration-1-0-0 http://www.eiffel.com/developers/xml/configuration-1-0-0.xsd" name="carbon_api" uuid="D8DA2332-0232-4B5D-A6D3-6605880F7D99" library_target="carbon_api">
	<target name="carbon_api">
	
		<file_rule>
			<exclude>/\.svn</exclude>
			<exclude>/EIFGEN.{0,1}$</exclude>
			<exclude>/tmp$</exclude>
			<exclude>/temp$</exclude>
			<exclude>/release$</exclude>
			<exclude>/obsolete$</exclude>
		</file_rule>
		
		<root all_classes="true"/>
		<option warning="true"/>
		<library name="base" location="$ISE_LIBRARY/library/base/base.ecf" readonly="True"/>
		<library name="time" location="$ISE_LIBRARY/library/time/time.ecf" readonly="True"/>
		<external_include location="${ISE_LIBRARY}/library/vision2/implementation/carbon/wrapper/generated_wrapper/c/include" />
		<external_include location="${ISE_LIBRARY}/library/vision2/implementation/carbon/wrapper/generated_wrapper/c/include/spec/ise -isysroot /Developer/SDKs/MacOSX10.4u.sdk" />
		<external_object location="${ISE_LIBRARY}/library/vision2/implementation/carbon/wrapper/carbon_c_eiffel_glue_library_ise_gcc.a -framework Carbon" />
		<cluster name="ewg" location="${EWG}\library\runtime\eiffel\">
			<cluster name="ewg_spec" location="$|spec">
				<cluster name="ewg_spec_ise" location="$|ise"/>
			</cluster>
		</cluster>
		<cluster name="gobo" location="${GOBO}\library\">
			<cluster name="kl_kernel" location="$|kernel\">
				<cluster name="kl_basic" location="$|basic\"/>
				<cluster name="kl_elks" location="$|elks\"/>
				<cluster name="kl_io" location="$|io\"/>
				<cluster name="kl_misc" location="$|misc\"/>
				<cluster name="kl_spec" location="$|spec\">
					<cluster name="kl_spec_ise" location="$|ise\"/>
				</cluster>
				<cluster name="kl_support" location="$|support\"/>
				<cluster name="kl_unicode" location="$|unicode\"/>
			</cluster>
		</cluster>
		<cluster name="carbon" location="generated_wrapper\eiffel\">
			<cluster name="carbon_abstraction_abstraction" location="$|abstraction\">
				<cluster name="carbon_abstraction_callback" location="$|callback\"/>
				<cluster name="carbon_abstraction_function" location="$|function\"/>
				<cluster name="carbon_abstraction_struct" location="$|struct\"/>
				<cluster name="carbon_abstraction_union" location="$|union\"/>
				<cluster name="carbon_abstraction_enum" location="$|enum\"/>
			</cluster>
			<cluster name="carbon_external_external" location="$|external\">
				<cluster name="carbon_external_enum" location="$|enum\">
					<cluster name="carbon_external_enum_spec" location="$|spec\">
						<cluster name="carbon_external_enum_ise" location="$|ise\"/>
					</cluster>
				</cluster>
				<cluster name="carbon_external_function" location="$|function\">
					<cluster name="carbon_external_function_spec" location="$|spec\">
						<cluster name="carbon_external_function_ise" location="$|ise\"/>
					</cluster>
				</cluster>
				<cluster name="carbon_external_struct" location="$|struct\">
					<cluster name="carbon_external_struct_spec" location="$|spec\">
						<cluster name="carbon_external_struct_ise" location="$|ise\"/>
					</cluster>
				</cluster>
				<cluster name="carbon_external_union" location="$|union\">
					<cluster name="carbon_external_union_spec" location="$|spec\">
						<cluster name="carbon_external_union_ise" location="$|ise\"/>
					</cluster>
				</cluster>
			</cluster>
		</cluster>
		<cluster name="Additional_wrappers" location="manual_wrapper" recursive="true" />
	</target>
</system>
